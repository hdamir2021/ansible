[demo_servers]
centos01   ansible_host=10.0.0.21
ubuntu01   ansible_host=10.0.0.31

[centos]
centos01   ansible_host=10.0.0.21 ansible_ssh_user=vagrant ansible_ssh_pass=vagrant

[ubuntu]
ubuntu01   ansible_host=10.0.0.31

[demo_servers:vars]
ansible_ssh_user=vagrant

[ubuntu:vars]
ansible_ssh_user=ubuntu 
ansible_ssh_pass=ubuntu
